{
  "id": "1",
  "name": "azurecost-backend",
  "version": "1.0.11",
  "description": "Azure cost backend",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "DfE",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@grafana/e2e": "10.3.1",
    "@grafana/e2e-selectors": "10.3.1",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.4.0",
    "@swc/helpers": "^0.5.6",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.70.0",
    "sass-loader": "14.1.0",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@cypress/request": "^3.0.1",
    "@emotion/css": "^11.1.3",
    "@grafana/data": "10.0.3",
    "@grafana/runtime": "10.0.3",
    "@grafana/ui": "10.0.3",
    "debug": "^4.3.4",
    "minimatch": "^9.0.3",
    "nanoid": "^5.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "request": "^2.88.2",
    "tough-cookie": "^4.1.3",
    "tslib": "2.6.2",
    "underscore": "^1.13.6"
  },
  "packageManager": "npm@10.1.0"
}
